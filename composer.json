{
    "name": "laravel-mod/core",
    "description": "Lite modular package for Laravel",
    "keywords": [
        "modules",
        "laravel",
        "mod",
        "modular",
        "package"
    ],
    "homepage": "https://github.com/Luthfiahmad12/laravel-mod",
    "license": "MIT",
    "authors": [
        {
            "name": "Achmad Luthfi Afif",
            "email": "luthfiafif625@gmail.com",
            "homepage": "https://github.com/Luthfiahmad12",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMod\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelMod\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMod\\Providers\\ModServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/Luthfiahmad12"
        }
    ]
}
