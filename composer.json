{
    "name": "laravel-mod/core",
    "description": "Lite modular package for Laravel",
    "keywords": [
        "modules",
        "laravel",
        "mod",
        "modular"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Achmad Luthfi Afif",
            "email": "luthfiafif625@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/framework": "^12.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMod\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMod\\Providers\\ModServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
